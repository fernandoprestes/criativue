{
  "name": "criativue",
  "private": true,
  "type": "module",
  "version": "0.3.0",
  "author": {
    "name": "fernandoprestes",
    "url": "https://github.com/fernandoprestes"
  },
  "scripts": {
    "dev": "vite --open",
    "dev:host": "vite --host --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore",
    "stylelint": "stylelint \"src/**/*.{vue,scss}\" --fix",
    "stylelint:check": "stylelint \"src/**/*.{vue,scss}\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*src/**/*.{js,ts,vue}": [
      "eslint --fix --ignore-path .gitignore",
      "prettier --write"
    ],
    "*src/**/*.{css,scss,vue}": [
      "prettier --write",
      "stylelint --fix "
    ]
  },
  "dependencies": {
    "@iconify-prerendered/vue-mdi": "^0.12.1663305505",
    "@vueuse/core": "^9.2.0",
    "vee-validate": "^4.7.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@hint/connector-local": "^3.2.24",
    "@hint/formatter-codeframe": "^3.1.33",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "hint": "^7.1.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-sass-render-errors": "^3.0.0",
    "stylelint-selector-no-empty": "^1.0.9",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9"
  }
}
