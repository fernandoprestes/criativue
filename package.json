{
  "name": "criativue",
  "private": true,
  "type": "module",
  "version": "0.3.0",
  "author": {
    "name": "fernandoprestes",
    "url": "https://github.com/fernandoprestes"
  },
  "scripts": {
    "dev": "vite --open",
    "dev:host": "vite --host --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext \"src/**/*.{vue,ts,js}\" --fix --ignore-path .gitignore",
    "stylelint": "stylelint \"src/**/*.{vue,scss}\" --fix",
    "stylelint:check": "stylelint \"src/**/*.{vue,scss}\"",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*src/**/*.{js,ts,vue}": [
      "eslint --fix --ignore-path .gitignore",
      "prettier --write"
    ],
    "*src/**/*.{css,scss,vue}": [
      "prettier --write",
      "stylelint --fix "
    ]
  },
  "dependencies": {
    "@iconify-prerendered/vue-mdi": "^0.17.1671435623",
    "@vueuse/core": "^9.9.0",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.1.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.1.0",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-sass-render-errors": "^3.0.0",
    "stylelint-selector-no-empty": "^1.0.9",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vitest": "^0.26.2",
    "vue-tsc": "^1.0.16"
  }
}
